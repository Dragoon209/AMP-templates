App.ApplicationIPBinding=0.0.0.0
App.Ports=[{"Protocol":"TCP","Port":14159,"Ref":"GamePort","Name":"Game Port","Description":"Port for game traffic"}]
App.DisplayName=Necesse
App.MaxUsers=20
App.RootDir=./necesse/
App.BaseDirectory=./necesse/1169370/
App.ExecutableLinux=1169370/./jre/bin/java
App.ExecutableWin=1169370\.\jre\bin\java.exe
App.WorkingDir=1169370
App.EnvironmentVariables={"LD_LIBRARY_PATH": "./linux64:%LD_LIBRARY_PATH%", "SteamAppId": "1169370"}
